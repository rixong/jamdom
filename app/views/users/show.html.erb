<h2><%= @user.name%> </h2>

<h4> Instrument(s): </h4>
<ul>
<% @user.instruments.each do |inst| %>
    <li><%= inst.name%></li>
<% end %>
</ul>

<h4> Genre(s): </h4>
<ul>
<% @user.genres.each do |gen| %>
    <li><%= gen.name%></li>
<% end %>
</ul>

<h4> Your Jams:</h4>
<ul>
<% @user.jams.each do |jam| %>
    <li><%= link_to jam.name, jam_path(jam)%></li>
<% end %>
</ul>




